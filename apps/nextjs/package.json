    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/supabase-js": "^2.23.0",
    "next": "13.4.12",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "^1.0.0",
